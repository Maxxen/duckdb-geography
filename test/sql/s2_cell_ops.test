# name: test/sql/s2_cell_ops.test
# description: test s2 extension cell operations
# group: [s2]

# Require statement will ensure this test is run with this extension loaded
require s2

query I
SELECT 'POINT (-64 45)'::GEOGRAPHY::S2_CELL::VARCHAR;
----
